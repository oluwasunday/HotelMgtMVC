
@if(ViewBag.LogInfo != null)
{
    <li class="nav-item">Welcome @ViewBag.Username</li>
    <li class="nav-item"><a class="nav-link" asp-action="Register" asp-controller="Authentication">Logout</a></li>
}
else
{
    <li class="nav-item"><a class="nav-link" asp-action="Login" asp-controller="Authentication">Login</a></li>
    <li class="nav-item"><a class="nav-link" asp-action="Register" asp-controller="Authentication">Register</a></li>
}

